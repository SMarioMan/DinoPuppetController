<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1d4851"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.led_on_off.led.dinoControl">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Dino Controller"
        android:textColor="#ffffff"
        android:textSize="20dp" />

    <SeekBar
        android:id="@+id/eyes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/up"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/up2"
        android:layout_toLeftOf="@+id/up2"
        android:layout_toStartOf="@+id/up2" />

    <SeekBar
        android:id="@+id/jaw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/up"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/up2"
        android:layout_toEndOf="@+id/up2"
        android:layout_toRightOf="@+id/up2" />

    <Button
        android:id="@+id/up2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_above="@+id/up"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Up2" />

    <Button
        android:id="@+id/up"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_above="@+id/left"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Up" />

    <Button
        android:id="@+id/left"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Left" />

    <Button
        android:id="@+id/right"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Right" />

    <Button
        android:id="@+id/blink"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Blink"
        android:layout_alignTop="@+id/discnt"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/down"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/left"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Down" />

    <Button
        android:id="@+id/down2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/down"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@color/primary_material_dark"
        android:text="Down2" />

    <ImageButton
        android:id="@+id/discnt"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@null"
        android:src="@mipmap/back" />

    <ImageButton
        android:id="@+id/abt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@null"
        android:onClick="about"
        android:src="@drawable/about" />

</RelativeLayout>
